{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@meld/scaffold": [
        "dist/scaffold"
      ],
      "@meld/scaffold/*": [
        "dist/scaffold/*"
      ],
      "@meld/app-bar": [
        "dist/app-bar"
      ],
      "@meld/app-bar/*": [
        "dist/app-bar/*"
      ]
    }
  }
}
